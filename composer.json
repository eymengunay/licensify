{
    "name":        "eo/licensify",
    "type":        "library",
    "description": "Automatically add license headers to your PHP source files",
    "keywords":    ["license", "licensify"],
    "homepage":    "https://github.com/eymengunay/licensify",
    "license":     "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "~2.3",
        "symfony/finder": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "Eo\\Licensify\\": "src/"
        }
    },
    "bin": ["bin/licensify"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
